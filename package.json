{
  "version": "1.0.0",
  "description": "A modern, efficient pathfinding application for real-world routing and navigation challenges",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn src/index.ts",
    "build": "tsc && cp -r src/dist/* dist/ 2>/dev/null || echo 'No dist files to copy'",
    "start": "node dist/index.js",
    "start:test": "NODE_ENV=test PORT=3001 node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "vitest run test/e2e",
    "test:unit": "vitest run test/unit",
    "test:playwright": "playwright test",
    "test:firestore-rules": "firebase emulators:exec --only firestore 'npm run test:unit'",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "prepare": "husky install",
    "setup:cloud": "./scripts/setup-cloud.sh",
    "setup:firebase": "./scripts/setup-firebase.sh",
    "setup:all": "npm run setup:cloud && npm run setup:firebase",
    "verify:setup": "./scripts/verify-setup.sh",
    "deploy:rules": "firebase deploy --only firestore:rules,firestore:indexes,storage",
    "deploy:hosting": "npm run build && firebase deploy --only hosting",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:cloud-run": "./scripts/deploy-cloud-run.sh",
    "deploy:all": "npm run build && firebase deploy",
    "emulators:start": "firebase emulators:start",
    "emulators:setup": "./scripts/setup-firebase.sh --emulators-only"
  },
  "keywords": [
    "pathfinding",
    "visualization",
    "api",
    "algorithms"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/aiplatform": "^5.6.0",
    "@google/generative-ai": "^0.24.1",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "firebase-admin": "^11.5.0",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "rate-limiter-flexible": "^7.2.0",
    "redis": "^4.6.7",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.19.11",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3",
    "vitest": "^0.34.6"
  },
  "eslintConfig": {
    "rules": {
      "prettier/prettier": "error",
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/explicit-function-return-type": "warn"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2
  }
}
