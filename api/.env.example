# Environment Configuration
NODE_ENV=development
PORT=8080

# Google Cloud Configuration
GCLOUD_PROJECT=demo-project-id
GCP_REGION=asia-south1
VERTEX_MODEL=gemini-1.5-pro
# GOOGLE_APPLICATION_CREDENTIALS=./pathfinder-sa.json  # Commented out for local development

# Firebase Configuration
FIREBASE_PROJECT_ID=demo-project-id

# API Keys (use dummy values for local development)
GOOGLE_AI_API_KEY=demo-google-ai-api-key
GOOGLE_MAPS_API_KEY=demo-google-maps-api-key

# Security Configuration
CORS_ORIGIN=http://localhost:3000,http://localhost:5173
RATE_LIMIT_WINDOW_MS=900000
